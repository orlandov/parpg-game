<?xml version="1.0" encoding="ascii"?>
<!-- PC, NPC and objects initial data for PARPG maps -->
<!-- All data that is subject to change by the engine during game time
     should be stored in this file -->
<objects format="1.0">
    <!-- There must be one and one only PC character -->
    <PC xpos="0.0" ypos="0.0"></PC>
    <!-- There can be any number of NPC characters -->
    <NPC gfx="npc-woman" xpos="-4.0" ypos="-7.0" id="women01"
         text="A friendly woman"></NPC>
    <!-- There can be any number of objects -->
    <!-- Object blocking is set in the objects own xml file -->
    <object display="True" gfx="crate" xpos="-3.0" ypos="-4.0" id="crate01"
            carry="0" contain="1" text="Just a crate"></object>
    <object display="True" gfx="crate" xpos="-4.0" ypos="-6.0" id="crate02"
            carry="0" contain="1" text="A dirty old crate"></object>
    <object display="False" gfx="NONE" owner="crate02" id="letter"
            carry="1" contain="0" text="A newly thrown-away letter"></object>
    <object display="False" gfx="NONE" owner="crate02" id="battery"
            carry="1" contain="0" text="An old battery"></object>
		<!-- There can be any number of 'trasition tiles', or tiles that move the
		PC around -->
		<tele_tile target="map2" xpos="-14" ypos="10" txpos="-14" typos="-17">
						</tele_tile>
</objects>

